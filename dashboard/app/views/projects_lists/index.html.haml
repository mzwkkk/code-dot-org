- content_for :head do
  %script{src:'/assets/js/projectsList.js'}

%h1= "Projects for section #{params[:section_id]}"
#projects-list

:ruby
  reactProps = {
    projectsData: @section_projects
  }

:javascript
  ReactDOM.render(React.createElement(window.dashboard.ProjectsList, #{reactProps.to_json}),
    document.getElementById('projects-list'));
